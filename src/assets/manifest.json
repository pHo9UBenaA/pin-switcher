{
  "manifest_version": 3,
  "name": "Pin Switcher",
  "short_name": "Pin Switcher",
  "version": "0.1.0",
  "description": "Toggle active tab pinning/unpinning with shortcuts.",
  "author": "pHo9UBenaA",
  "action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+I",
        "mac": "Command+I"
      },
      "description": "Triggering pin/unpin"
    }
  },
  "permissions": [],
  "incognito": "spanning"
}
